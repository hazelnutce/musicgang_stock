{
    "name": "musicgang_stock",
    "main": "electron-starter.js",
    "version": "1.0.0",
    "description": "simple stock for easier life",
    "engine": {
        "node": "8.1.1",
        "npm": "5.0.3"
    },
    "scripts": {
        "start": "node index.js",
        "server": "nodemon index.js",
        "client": "npm run start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
        "electron": "set ELECTRON_START_URL=http://localhost:3000 && set NODE_ENV=development && electron .",
        "electron-production": "npm run build --prefix client && electron .",
        "electron-dev": "nf start",
        "package": "electron-packager . ms_test"
    },
    "keywords": [
        "musicgang"
    ],
    "author": "hazelnut",
    "license": "ISC",
    "dependencies": {
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.18.3",
        "child_process": "^1.0.2",
        "concurrently": "^3.6.0",
        "cookie-session": "^2.0.0-beta.3",
        "cross-env": "^5.2.0",
        "crypto": "^1.0.1",
        "crypto-password-helper": "0.0.3",
        "electron-packager": "^12.1.1",
        "express": "^4.16.3",
        "jquery": "^3.3.1",
        "keymaster": "^1.6.2",
        "lokijs": "^1.5.5",
        "materialize-css": "^1.0.0-rc.2",
        "mongoose": "^5.2.1",
        "nodemon": "^1.17.5",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "request": "^2.88.0"
    },
    "devDependencies": {
        "asar": "^0.14.3",
        "electron": "^2.0.8",
        "foreman": "^3.0.1"
    }
}
